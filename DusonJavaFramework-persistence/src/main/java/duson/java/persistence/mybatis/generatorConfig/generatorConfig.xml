<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  <!-- <properties resource="generatorConfig.properties" /> -->
  <classPathEntry location="mysql-connector-java-5.1.38.jar"/>
  <context id="context1" >
	<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">  
	    <property name="searchString" value="Example$" />  
	    <property name="replaceString" value="Criteria" />  
	</plugin>
	
	<commentGenerator>  
       <property name="suppressAllComments" value="true" />  
    </commentGenerator> 
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://:3306/database" userId="root" password="" />
    
    <javaModelGenerator targetPackage="实体类包名" targetProject="目标项目名或路径">
    	<property name="trimStrings" value="true" />
    </javaModelGenerator>
    <sqlMapGenerator targetPackage="持久层Mapper包名" targetProject="目标项目名或路径">
    	<property name="trimStrings" value="true" />
    </sqlMapGenerator>
    <javaClientGenerator targetPackage="包名" type="XMLMAPPER" targetProject="目标项目名或路径">
    	<property name="trimStrings" value="true" />
    </javaClientGenerator>

    <table schema="数据库名" tableName="表名" domainObjectName="Java类名" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
      <property name="useActualColumnNames" value="true" /> 
    </table>
     
  </context>
</generatorConfiguration>